{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "pipeline-test",
      "links": [
        "postgres"
      ],
      "image": "cyjay96/pipeline-test",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 80
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://postgres:5432/pipeline_test_db"
        }
      ],
      "memory": 512
    },
    {
      "name": "postgres",
      "image": "postgres:13.5",
      "essential": true,
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_DB",
          "value": "pipeline_test_db"
        }
      ],
      "memory": 512
    }
  ]
}
